import java.util.Scanner;

public class Tabuada {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Digite um n√∫mero: ");
        int num = sc.nextInt();

        for (int i = 0; i < 10; i++) {
            int result = num * i;
            System.out.println(num + " X " + i + " = " + result);
        }
    }
}
